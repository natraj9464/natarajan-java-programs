import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        
        int a[]=new int[10];
        int n=s.nextInt();
        int sum = 0;
        
        for(int i = 0; i < n; i++){
            a[i] = s.nextInt();
            sum =sum+a[i];
        }
        
        System.out.println(sum);
        
    }
}
