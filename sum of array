import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        int N = scanner.nextInt();
        int sum = 0;
        
        for(int i = 0; i < N; i++){
            int num = scanner.nextInt();
            sum += num;
        }
        
        System.out.println(sum);
        
        
    }
}
