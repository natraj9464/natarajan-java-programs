import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String S=s.nextLine();
        int c=s.nextInt();
        int b=s.nextInt();
        StringBuilder sb=new StringBuilder(S);
        sb.delete(c,b);
        System.out.println(sb);
    }
}
