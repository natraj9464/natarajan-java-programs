import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        String S=s.nextLine();
        String f=s.nextLine();
        int n=s.nextInt();
        StringBuilder sb=new StringBuilder(S);
        sb.insert(n,f);
        System.out.println(sb);
    }
}
