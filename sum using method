import java.io.*;
import java.util.*;
class Solution {
    public void sum(int a,int b)
    {
        int c=a+b;
        System.out.println(c);
    }
public static void main(String[] args){
    Scanner s=new Scanner(System.in);
    int a=s.nextInt();
    int b=s.nextInt(); 
    Solution ob=new Solution();
    ob.sum(a,b);
   
    }
}
